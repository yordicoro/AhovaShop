{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "front-office": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/front-office",
      "sourceRoot": "projects/front-office/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/front-office/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/front-office/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/front-office/public"
              }
            ],
            "styles": [
              "projects/front-office/src/styles.css"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "front-office:build:production"
            },
            "development": {
              "buildTarget": "front-office:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/front-office/tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/front-office/public"
              }
            ],
            "styles": [
              "projects/front-office/src/styles.css"
            ]
          }
        }
      }
    },
    "back-office": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/back-office",
      "sourceRoot": "projects/back-office/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/back-office/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/back-office/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/back-office/public"
              }
            ],
            "styles": [
              "projects/back-office/src/styles.css"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "back-office:build:production"
            },
            "development": {
              "buildTarget": "back-office:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/back-office/tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/back-office/public"
              }
            ],
            "styles": [
              "projects/back-office/src/styles.css"
            ]
          }
        }
      }
    },
    "clothing-core": {
      "projectType": "library",
      "root": "projects/clothing-core",
      "sourceRoot": "projects/clothing-core/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/clothing-core/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/clothing-core/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/clothing-core/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "afc9b647-42e2-46eb-8977-f2ce876ebc81"
  }
}
